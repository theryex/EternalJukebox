# Floppa Analyzer Environment Variables
# Copy this to .env and fill in your values

# Spotify API credentials (get from https://developer.spotify.com/dashboard)
SPOTIPY_CLIENT_ID=
SPOTIPY_CLIENT_SECRET=
SPOTIPY_REDIRECT_URI=http://localhost:8888/callback

# Output directories (these should match the volume mounts in docker-compose)
JUKEBOX_ANALYSIS_DIR=/data/uploaded_analysis/
JUKEBOX_AUDIO_DIR=/data/audio/
